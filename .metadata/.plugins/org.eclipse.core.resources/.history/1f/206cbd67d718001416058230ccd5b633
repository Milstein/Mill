package nineMensMorris;

public class NineGame extends Game {
	//0 for no piece, 1 for player 1, 2 for player 2
	private int[] slots;
	
	public NineGame() {
		setDisplay(new NineGameDisplay(this));
		slots = new int[24];
	}
	
	protected int getSlot(int index) {
		if (!(0<=index && index<=23))
			
		return slots[index];
	}
}
